            <div class="span12">
                <h3 class="page-title">
                    Report
                    <small>report</small>
                </h3>
                <ul class="breadcrumb">
                    <li>
                        <i class="icon-home"></i>
                        <a href="index.html">Home</a> 
                        <i class="icon-angle-right"></i>
                    </li>
                    <li><a href="#">Report</a></li>
                </ul>
                <!-- END PAGE TITLE & BREADCRUMB-->
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
               {% block main_body %}{% endblock %}
                
            </div>
        </div>
    </div>            
{% endblock %}

