{% extends "::base.html.twig" %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/pages/profile.css') }}" />
{% endblock%}    
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('js/profile.js') }}"></script>
